{
  "name": "portal",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings 0",
    "check": "clear && tsc --noEmit",
    "kill-ports": "lsof -ti:3000 | xargs kill -9 2>/dev/null || true",
    "dev:next": "next dev",
    "dev:turbo": "next dev --turbopack --port 3000",
    "tree": "clear && tree -I 'node_modules|.next|dist|build|out|coverage|.git|.cache|.turbo|@docs' -P '*.ts|*.tsx|*.js|*.jsx|*.html|*.json|*.md' -L 5",
    "test": "vitest",
    "test:run": "vitest run",
    "test:watch": "vitest --watch",
    "commit": "npx openrouter-commit -run",
    "commit2": "git add . && git commit -m 'fixes' && git push"
  },
  "dependencies": {
    "@carapis/api": "latest",
    "@carapis/nextjs": "latest",
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.11.1",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^5.1.1",
    "@iconify/react": "^6.0.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/lab": "7.0.0-beta.14",
    "@mui/material": "^7.1.1",
    "@mui/material-nextjs": "^7.1.1",
    "@mui/system": "^7.1.1",
    "@mui/utils": "^7.1.1",
    "@mui/x-date-pickers": "^7.15.0",
    "@react-spring/web": "^9.7.2",
    "@tanstack/query-sync-storage-persister": "^5.81.5",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.81.5",
    "@tanstack/react-query-persist-client": "^5.81.5",
    "@types/js-yaml": "^4.0.8",
    "axios": "^1.10.0",
    "chalk": "^4.1.2",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "colord": "^2.9.3",
    "dayjs": "^1.11.13",
    "js-yaml": "^4.1.0",
    "konva": "^9.3.20",
    "loglevel": "^1.9.2",
    "material-react-table": "^3.2.1",
    "moment": "^2.30.1",
    "next": "14.2.15",
    "next-mdx-remote": "^5.0.0",
    "notistack": "^3.0.2",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.0",
    "react-ga4": "^2.1.0",
    "react-hook-form": "^7.59.0",
    "react-hotkeys-hook": "^5.1.0",
    "react-json-tree": "^0.20.0",
    "react-konva": "^19.0.7",
    "react-lottie-player": "^2.1.0",
    "react-spring": "^9.7.2",
    "react-virtuoso": "^4.13.0",
    "slugify": "^1.6.6",
    "swiper": "^11.2.10",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.17",
